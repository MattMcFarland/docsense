{
  "licenser.projectName": "DocSense",
  "editor.tabSize": 2,
  "files.associations": {
    ".docsenserc": "json",
    ".prettierrc": "json"
  },
  "editor.rulers": [90],
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/node_modules/**": true,
    "**/dist/**": true,
    "**/docs/**": true,
    "**/.history/**": true
  },
  "search.exclude": {
    "**/node_modules": true,
    "**/bower_components": true,
    "**/dist": true,
    "**/.history/**": true,
    "**/types": true
  },
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/node_modules": true,
    "**/.history/**": true,
    "**/dist": true,
    "**/types": true
  },
  "editor.formatOnSave": true,
  "tslint.enable": true,
  "tslint.packageManager": "yarn",
  "typescript.referencesCodeLens.enabled": true,
  "typescript.useCodeSnippetsOnMethodSuggest": true
}
