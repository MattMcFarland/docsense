{
  "licenser.projectName": "DocSense",
  "editor.tabSize": 2,
  "files.associations": {
    ".docsenserc": "json",
    ".prettierrc": "json"
  },
  "editor.rulers": [90],
  "prettier.disableLanguages": ["md", "markdown", "hbs", "handlebars", "html"],
  "html.format.indentHandlebars": true,
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/node_modules/**": true,
    "**/dist/**": true,
    "**/docs/**": true,
    "**/types/**": true,
    "**/.history/**": true
  },
  "search.exclude": {
    "**/node_modules": true,
    "**/bower_components": true,
    "**/dist": true,
    "**/.history/**": true,
    "**/types": true,
    "**/docs": true
  },
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/node_modules": true,
    "**/.history/**": true,
    "**/dist/**": true,
    "**/docs/**": true,
    "**/types/**": true,
    "yarn*": true,
    ".gitattributes": true
  },
  "editor.formatOnSave": true,
  "tslint.enable": true,
  "tslint.packageManager": "yarn",
  "typescript.useCodeSnippetsOnMethodSuggest": true,
  "json.schemas": [
    {
      "fileMatch": [".docsenserc", "docsense.config.json"],
      "url": "./docsense.schema.json"
    }
  ],
  "json.personalSchemas": [
    {
      "fileMatch": [".docsenserc", "docsense.config.json"],
      "url": "./docsense.schema.json"
    }
  ],
  "autolaunch.config": [
    {
      "type": "task",
      "name": "watch"
    },
    {
      "type": "task",
      "name": "docsense-serve"
    }
  ],
  "typescript.tsdk": "node_modules\\typescript\\lib"
}
