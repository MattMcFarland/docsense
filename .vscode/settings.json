{
  "licenser.projectName": "DocSense",
  "javascript.validate.enable": false,
  "typescript.validate.enable": false,
  "flow.enabled": true,
  "editor.tabSize": 2,
  // Turns auto fix on save on or off.
  "eslint.autoFixOnSave": true,
  "eslint.packageManager": "yarn",
  "files.associations": {
    ".docsenserc": "json",
    ".prettierrc": "json"
  },
  "editor.rulers": [90],
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/node_modules/**": true,
    "**/lib/**": true,
    "**/docs/**": true
  },
  "search.exclude": {
    "**/node_modules": true,
    "**/bower_components": true,
    "**/lib": true
  },
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/node_modules": true,
    "**/lib": true
  },
  "editor.formatOnSave": true
}
