{
  "licenser.projectName": "DocSense",
  "editor.tabSize": 2,
  "files.associations": {
    ".docsenserc": "json",
    ".prettierrc": "json"
  },
  "editor.rulers": [90],
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/node_modules/**": true,
    "**/dist/**": true,
    "**/docs/**": true,
    "**/types": true,
    "**/.history/**": true
  },
  "search.exclude": {
    "**/node_modules": true,
    "**/bower_components": true,
    "**/dist": true,
    "**/.history/**": true,
    "**/types": true,
    "**/docs": true
  },
  "files.exclude": {
    "**/.git": false,
    "**/.svn": false,
    "**/.hg": false,
    "**/CVS": false,
    "**/.DS_Store": false,
    "**/node_modules": false,
    "**/.history/**": false,
    "**/dist": false,
    "**/docs": false,
    "**/types": false,
    "yarn*": false,
    ".gitattributes": false
  },
  "editor.formatOnSave": true,
  "tslint.enable": true,
  "tslint.packageManager": "yarn",
  "typescript.useCodeSnippetsOnMethodSuggest": true
}
