{
  "name": "docsense",
  "version": "0.0.0-1",
  "description":
    "WIP - Beautiful ES Doc generation with or without code comments",
  "main": "lib/index.js",
  "scripts": {
    "test": "jest",
    "test:debug":
      "node -r @babel/register --inspect-brk=9229 --nolazy node_modules/jest/bin/jest.js",
    "precommit": "yarn lint && lint-staged",
    "clean": "rm -rf out docs lib && mkdir docs lib",
    "prebuild": "yarn lint && flow check && yarn clean",
    "build":
      "babel src --out-dir lib --ignore __tests__,spec.js,test.js --copy-files",
    "docs": "node lib/index.js",
    "debug": "node -r @babel/register --inspect-brk=9229 --nolazy src/index.js",
    "prepublishOnly": "yarn build && yarn docs && pkg-ok",
    "prettier":
      "prettier-eslint --write src/**/*.{js,json,prettierrc,babelrc,docsenserc}",
    "lint": "eslint src/**/*.js"
  },
  "lint-staged": {
    "*.{js,json,prettierrc,babelrc,docsenserc}": [
      "prettier-eslint --write",
      "git add"
    ]
  },
  "jest": {
    "testMatch": ["**/__tests__/**/*.test.js"],
    "testPathIgnorePatterns": ["/lib/", "/node_modules/"],
    "reporters": ["jest-tap-reporter"]
  },
  "keywords": [],
  "author": "Matt McFarland <contact@mattmcfarland.com>",
  "license": "MIT",
  "dependencies": {
    "babylon": "7.0.0-beta.32",
    "cosmiconfig": "^3.1.0",
    "deepmerge": "^2.0.1",
    "doctrine": "^2.0.0",
    "eslint-plugin-flowtype": "^2.39.1",
    "glob": "^7.1.2",
    "json-query": "^2.2.2",
    "lowdb": "^1.0.0",
    "npmlog": "^4.1.2",
    "ramda": "^0.25.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.32",
    "@babel/core": "^7.0.0-beta.32",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0-beta.32",
    "@babel/preset-env": "^7.0.0-beta.32",
    "@babel/preset-flow": "^7.0.0-beta.32",
    "@babel/register": "^7.0.0-beta.32",
    "@types/doctrine": "^0.0.3",
    "babel-core": "^7.0.0-0",
    "babel-eslint": "^8.0.2",
    "babel-jest": "^21.2.0",
    "eslint": "^4.12.0",
    "flow-bin": "^0.59.0",
    "husky": "^0.14.3",
    "jest": "^21.2.1",
    "jest-tap-reporter": "^1.7.0",
    "lint-staged": "^5.0.0",
    "pkg-ok": "^1.1.0",
    "prettier-eslint": "^8.2.2",
    "prettier-eslint-cli": "^4.4.0"
  },
  "directories": {
    "lib": "lib"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MattMcFarland/docsense.git"
  },
  "bugs": {
    "url": "https://github.com/MattMcFarland/docsense/issues"
  },
  "homepage": "https://github.com/MattMcFarland/docsense#readme"
}
