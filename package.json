{
  "name": "docsense",
  "version": "0.0.0-1",
  "description":
    "WIP - Beautiful ES Doc generation with or without code comments",
  "main": "dist/common/index.js",
  "module": "dist/esm/index.js",
  "unpkg": "dist/docsense.js",
  "jsdelivr": "dist/docsense.js",
  "typings": "types/index.d.ts",
  "schema": "docsense.schema.json",
  "files": ["src", "dist/*.js", "types/*.d.ts"],
  "scripts": {
    "test": "jest",
    "test:debug": "node --inspect-brk --nolazy node_modules/jest/bin/jest.js",
    "precommit": "lint-staged",
    "clean": "rm -rf dist",
    "prebuild": "yarn clean && mkdir dist",
    "build:schema":
      "typescript-json-schema ./tsconfig.json -o docsense.schema.json --ignoreErrors DocSenseConfig",
    "build:common": "tsc -p tsconfig.common.json",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build:umd": "tsc -p tsconfig.umd.json",
    "build:info": "node scripts/build-info",
    "build":
      "yarn build:info && yarn build:schema && yarn build:common && yarn build:esm",
    "predocs": "rm -rf docs",
    "docs": "mkdir docs && node dist/common/index.js",
    "debug": "ts-node --inspect-brk --nolazy src/index.ts",
    "prepublishOnly": "yarn build && yarn docs && pkg-ok",
    "prettier":
      "prettier --write src/**/*.{ts,js,json,prettierrc,babelrc,docsenserc}",
    "tslint-check": "tslint-config-prettier-check ./tslint.json",
    "poc": "ts-node src/generator/POC.ts",
    "predev-docs": "yarn build && yarn docs && yarn poc ",
    "dev-docs": "node scripts/dev-docs"
  },
  "lint-staged": {
    "*.{ts,js,json,prettierrc,babelrc,docsenserc}": [
      "prettier --write",
      "git add"
    ]
  },
  "keywords": [],
  "author": "Matt McFarland <contact@mattmcfarland.com>",
  "license": "MIT",
  "dependencies": {
    "babel-traverse": "^6.26.0",
    "babel-types": "^6.26.0",
    "babylon": "7.0.0-beta.32",
    "cosmiconfig": "^3.1.0",
    "deepmerge": "^2.0.1",
    "doctrine": "^2.0.0",
    "emoji": "^0.3.2",
    "from": "^0.1.7",
    "glob": "^7.1.2",
    "handlebars": "^4.0.11",
    "handlebars-helpers": "^0.10.0",
    "highlight.js": "^9.12.0",
    "highlightjs": "^9.10.0",
    "import": "^0.0.6",
    "lowdb": "^1.0.0",
    "marked": "^0.3.7",
    "mkdirp": "^0.5.1",
    "node-emoji": "^1.8.1",
    "npmlog": "^4.1.2",
    "pascal-case": "^2.0.1",
    "tachyons": "^4.9.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.32",
    "@babel/plugin-transform-typescript": "^7.0.0-beta.32",
    "@babel/preset-es2015": "^7.0.0-beta.32",
    "@babel/preset-typescript": "^7.0.0-beta.32",
    "@babel/register": "^7.0.0-beta.32",
    "@types/babel-traverse": "^6.25.3",
    "@types/babylon": "^6.16.2",
    "@types/deepmerge": "^1.3.3",
    "@types/doctrine": "^0.0.3",
    "@types/glob": "^5.0.33",
    "@types/handlebars": "^4.0.36",
    "@types/highlight.js": "^9.12.2",
    "@types/jest": "^21.1.8",
    "@types/lowdb": "^0.15.0",
    "@types/marked": "^0.3.0",
    "@types/mkdirp": "^0.5.2",
    "@types/node": "^8.0.53",
    "@types/node-emoji": "^1.4.0",
    "@types/ramda": "^0.25.6",
    "browser-sync": "^2.18.13",
    "eslint": "^4.12.0",
    "husky": "^0.14.3",
    "jest": "^21.2.1",
    "jest-spec-reporter": "^1.0.3",
    "jest-tap-reporter": "^1.7.0",
    "lint-staged": "^6.0.0",
    "nodegit": "^0.20.3",
    "pkg-ok": "^1.1.0",
    "prettier-eslint": "^8.2.2",
    "prettier-eslint-cli": "^4.4.0",
    "sane": "^2.2.0",
    "ts-jest": "^21.2.3",
    "ts-node": "^3.3.0",
    "tslib": "^1.8.0",
    "tslint": "^5.8.0",
    "tslint-config-prettier": "^1.6.0",
    "tslint-eslint-rules": "^4.1.1",
    "typescript": "^2.6.2",
    "typescript-babel-jest": "^1.0.5",
    "typescript-json-schema": "^0.20.0"
  },
  "jest": {
    "moduleFileExtensions": ["ts", "tsx", "js"],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "testMatch": ["**/__tests__/*.ts"],
    "testPathIgnorePatterns": ["/node_modules/", "/dist/", "/.history/"],
    "verbose": false
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MattMcFarland/docsense.git"
  },
  "bugs": {
    "url": "https://github.com/MattMcFarland/docsense/issues"
  },
  "homepage": "https://github.com/MattMcFarland/docsense#readme"
}
