{
  "name": "docsense",
  "version": "0.0.0-1",
  "description":
    "WIP - Beautiful ES Doc generation with or without code comments",
  "main": "dist_common/index.js",
  "module": "dist_esm/index.js",
  "files": ["dist_common/*", "dist_esm/*"],
  "scripts": {
    "test": "jest",
    "test:debug":
      "node -r ts-node/register --inspect-brk=9229 --nolazy node_modules/jest/bin/jest.js",
    "precommit": "yarn lint && lint-staged",
    "clean":
      "rm -rf docs dist_common dist_esm && mkdir docs dist_common dist_esm",
    "prebuild": "yarn clean",
    "build:common": "tsc -p tsconfig.common.json",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build": "yarn build:common && yarn build:esm",
    "docs": "node dist_common/index.js",
    "debug": "ts-node --inspect-brk=9229 --nolazy src/index.ts",
    "prepublishOnly": "yarn build && yarn docs && pkg-ok",
    "prettier":
      "prettier --write src/**/*.{ts,js,json,prettierrc,babelrc,docsenserc}",
    "lint": "tslint src/**/*.ts",
    "tslint-check": "tslint-config-prettier-check ./tslint.json"
  },
  "lint-staged": {
    "*.{ts,js,json,prettierrc,babelrc,docsenserc}": [
      "prettier --write",
      "git add"
    ]
  },
  "keywords": [],
  "author": "Matt McFarland <contact@mattmcfarland.com>",
  "license": "MIT",
  "dependencies": {
    "babel-traverse": "^6.26.0",
    "babel-types": "^6.26.0",
    "babylon": "7.0.0-beta.32",
    "cosmiconfig": "^3.1.0",
    "deepmerge": "^2.0.1",
    "doctrine": "^2.0.0",
    "glob": "^7.1.2",
    "json-query": "^2.2.2",
    "lowdb": "^1.0.0",
    "npmlog": "^4.1.2",
    "ramda": "^0.25.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.32",
    "@babel/plugin-transform-typescript": "^7.0.0-beta.32",
    "@babel/preset-es2015": "^7.0.0-beta.32",
    "@babel/preset-typescript": "^7.0.0-beta.32",
    "@babel/register": "^7.0.0-beta.32",
    "@types/babel-traverse": "^6.25.3",
    "@types/babylon": "^6.16.2",
    "@types/deepmerge": "^1.3.3",
    "@types/doctrine": "^0.0.3",
    "@types/glob": "^5.0.33",
    "@types/jest": "^21.1.8",
    "@types/lowdb": "^0.15.0",
    "@types/node": "^8.0.53",
    "eslint": "^4.12.0",
    "husky": "^0.14.3",
    "jest": "^21.2.1",
    "jest-tap-reporter": "^1.7.0",
    "lint-staged": "^5.0.0",
    "pkg-ok": "^1.1.0",
    "prettier-eslint": "^8.2.2",
    "prettier-eslint-cli": "^4.4.0",
    "ts-jest": "^21.2.3",
    "ts-node": "^3.3.0",
    "tslib": "^1.8.0",
    "tslint": "^5.8.0",
    "tslint-config-prettier": "^1.6.0",
    "tslint-eslint-rules": "^4.1.1",
    "typescript": "^2.6.2",
    "typescript-babel-jest": "^1.0.5"
  },
  "jest": {
    "moduleFileExtensions": ["ts", "tsx", "js"],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "testMatch": ["**/__tests__/*.ts"]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MattMcFarland/docsense.git"
  },
  "bugs": {
    "url": "https://github.com/MattMcFarland/docsense/issues"
  },
  "homepage": "https://github.com/MattMcFarland/docsense#readme"
}
