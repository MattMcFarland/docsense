# EditorConfig is awesome: http://EditorConfig.org

# top-most EditorConfig file
root = true


[*]
end_of_line = lf
insert_final_newline = true

[src/**.js]
indent_style = space
indent_size = 2

[src/**.ts]
indent_style = space
indent_size = 2

[.vscode/**.json]
indent_style = space
indent_size = 2

[{package.json,.babelrc, .docsenserc, .prettierrc, .eslintrc}]
indent_style = space
indent_size = 2
